<?xml version="1.0" encoding="UTF-8"?><sv:node xmlns:sv="http://www.jcp.org/jcr/sv/1.0" sv:name="hst:catalog">
  <sv:property sv:name="jcr:primaryType" sv:type="Name">
    <sv:value>hst:catalog</sv:value>
  </sv:property>
  <sv:node sv:name="hippoangular-catalog">
    <sv:property sv:name="jcr:primaryType" sv:type="Name">
      <sv:value>hst:containeritempackage</sv:value>
    </sv:property>
    <sv:node sv:name="list">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>hst:containeritemcomponent</sv:value>
      </sv:property>
      <sv:property sv:name="hst:componentclassname" sv:type="String">
        <sv:value>org.onehippo.example.components.ListView</sv:value>
      </sv:property>
      <sv:property sv:name="hst:iconpath" sv:type="String">
        <sv:value>images/catalog-list.png</sv:value>
      </sv:property>
      <sv:property sv:name="hst:label" sv:type="String">
        <sv:value>List</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parameternames" sv:type="String" sv:multiple="true">
        <sv:value>title</sv:value>
        <sv:value>pageSize</sv:value>
        <sv:value>sortBy</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parametervalues" sv:type="String" sv:multiple="true">
        <sv:value>List</sv:value>
        <sv:value>3</sv:value>
        <sv:value>hippoangular:date</sv:value>
      </sv:property>
      <sv:property sv:name="hst:template" sv:type="String">
        <sv:value>base.main.list</sv:value>
      </sv:property>
      <sv:property sv:name="hst:xtype" sv:type="String">
        <sv:value>HST.Item</sv:value>
      </sv:property>
    </sv:node>
    <sv:node sv:name="greeting">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>hst:containeritemcomponent</sv:value>
      </sv:property>
      <sv:property sv:name="hst:componentclassname" sv:type="String">
        <sv:value>org.onehippo.example.components.GreetingComponent</sv:value>
      </sv:property>
      <sv:property sv:name="hst:iconpath" sv:type="String">
        <sv:value>images/greeting_icon.png</sv:value>
      </sv:property>
      <sv:property sv:name="hst:label" sv:type="String">
        <sv:value>Greeting</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parameternames" sv:type="String" sv:multiple="true">
        <sv:value>description</sv:value>
        <sv:value>url</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parametervalues" sv:type="String" sv:multiple="true">
        <sv:value>Greetings REST applicatie</sv:value>
        <sv:value>http://localhost:8086/greeting</sv:value>
      </sv:property>
      <sv:property sv:name="hst:template" sv:type="String">
        <sv:value>home.main.app-include</sv:value>
      </sv:property>
      <sv:property sv:name="hst:xtype" sv:type="String">
        <sv:value>HST.Item</sv:value>
      </sv:property>
    </sv:node>
  </sv:node>
</sv:node>
